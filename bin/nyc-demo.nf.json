{
    "name": "NYC Taxi Yellow Ingest",
    "description": "Ingest NYC Taxi events from staging server.",
    "parameters": {
        "dir_input": "/tmp/demo/staged",
		"sdp_source_system": "NYC_STG",
		"sdp_source_stream": "NYC_YELLOW",
		"sdp_blob_storage_container": "nyctaxi"
    },
	"variables": {
	    "dir_input": "{dir_input}",
		"sdp_source_system":"{sdp_source_system}",
		"sdp_source_stream":"{sdp_source_stream}",
		"sdp_blob_storage_container": "{sdp_blob_storage_container}"
	},
    "properties": {
		"NYC Taxi Yellow Ingest/NYCYellow Store Files/PutAzureBlobStorage": {
			"storage-account-name": "${{sdp_blob_storage_account}}",
			"storage-account-key": "${{sdp_blob_storage_key}}"
		}
    },
    "pipeline": {
        "steps": [
            {
                "name": "NYCYellow Acquire from Staging",
                "type": "sdp_tpl_AcquireFiles",
                "description": "Acquire files from staging",
                "variables": {
                    "dir_input": null
                }
            },
            {
                "name": "NYCYellow Tag Files",
                "type": "sdp_tpl_Manage_TagFlowFiles",
                "variables": {
					"sdp_source_system": null,
					"sdp_source_stream": null
				}
            },
            {
                "name": "NYCYellow Unzip",
                "type": "sdp_tpl_Manage_Unzip"
            },
            {
                "name": "NYCYellow Store Files",
                "type": "sdp_tpl_StoreAzureBlob",
                "variables": {
					"sdp_dir_root": null,
					"sdp_blob_storage_account": null,
					"sdp_blob_storage_key": null,
					"sdp_blob_storage_container": null
				}
            }
        ]
    }
}